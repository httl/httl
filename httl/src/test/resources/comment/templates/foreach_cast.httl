<!--#var(List booklist)-->
<table>
  <!--#foreach(Book book in booklist)-->
  <tr>
    <td>${foreach.count}</td>
    <td>${book.title}</td>
    <td>${book.author}</td>
    <td>${book.publisher}</td>
